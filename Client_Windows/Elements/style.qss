/* Style of buttons in the sidebar  START  */


QPushButton#sidebar_button{
	text-align : left; 
	padding-left : 20; 	
	background-color : #131217 ; 
	color : #838383;
	border : 0 px;
	border-radius : 0px;
} 

QPushButton#sidebar_button::focus
{
	color : #23B2EE;
	border-right : 3px solid #B2FF59;
	background-color : #2B2C39 ;
}

QPushButton#sidebar_button::hover::focus
{
	color : #23B2EE;
	border-right : 3px solid #B2FF59;
	background-color : #2B2C39 ;
}

QPushButton#sidebar_button::pressed
{
	color : #23B2EE;
}

QPushButton#sidebar_button::hover
{
	color : #0892D0;
	border-right : 1px solid #AED581;
	background-color : #22232E ;
}
/* Style of buttons in the sidebar  END  */



/* Style of buttons in the close dialog box  START  */

QPushButton#close_button_yes{
	text-align : center;
	background-color : #090909 ; 
	color : #990000;
}

QPushButton#close_button_yes::focus{
	color: #AA0000;
}

QPushButton#close_button_yes::focus::hover{
	color: #FF0000;
}

QPushButton#close_button_yes::hover{
	color: #FF0000;
}



QPushButton#close_button_no{
	text-align : center;
	background-color : #090909 ;
	color : #009900;
}

QPushButton#close_button_no::focus{
	color: #00AA00;
}

QPushButton#close_button_no::focus::hover{
	color: #00FF00;
}

QPushButton#close_button_no::hover{
	color: #FF0000;
}

/* Style of buttons in the close dialog box  END  */



/* Style of buttons in the topbar  START  */
QPushButton#submit{
	font-size : 18px;
	text-align : center; 	
	border : 1px solid #ADFF2F;
	border-radius : 25px;
	background-color : #202428 ;
	color : #838383;
}

QPushButton#submit::focus
{
	color : #23B2EE;
	background-color : #2B2C39 ;
}

QPushButton#submit::hover::focus
{
	color : #23B2EE;
	background-color : #2B2C39 ;
}

QPushButton#submit::pressed
{
	color : #23B2EE;
	background-color : #202428;
}

QPushButton#submit::hover
{
	color : #0892D0;
	background-color : #202428 ;
}




QPushButton#ask{
	border-top-right-radius: 0px;
	border-bottom-right-radius: 25px;
	font-size : 18px;
	text-align : center; 	
	border : 1px solid #ADFF2F;
	background-color : #202428 ;
	color : #838383;
}

QPushButton#ask::hover::focus
{
	color : #23B2EE;
	background-color : #2B2C39 ;
}

QPushButton#ask::hover
{
	color : #0892D0;
	background-color : #202428 ;
}


QPushButton#problem_buttons{
	font-size : 24px;
	text-align : center; 	
	border : 1px solid #666666;
	margin-top : 50px;
	background-color : #1B1C25 ;
	color : #838383;

}

QPushButton#problem_buttons::focus
{
	color : #23B2EE;
	background-color : #2B2C39 ;
	border-right : 10px solid #E91E63;
}

QPushButton#problem_buttons::hover::focus
{
	color : #23B2EE;
	background-color : #2B2C39 ;
	border-right : 10px solid #E91E63;
}

QPushButton#problem_buttons::pressed
{
	color : #23B2EE;
	background-color : #202428;
}

QPushButton#problem_buttons::hover
{
	color : #0892D0;
	background-color : #22232E;
	border-right : 2px solid #E91E63;

}

/* Style of buttons in the topbar  END  */




