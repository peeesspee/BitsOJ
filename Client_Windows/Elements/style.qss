/* Table styles */

QTableView{
	background-color : #1D2227;
	alternate-background-color : #242A31;
	color : #AAAAAA;
	border : 0px;
	gridline-color : #transparent;
	selection-background-color : #171B1F;
}

QTableView::item:selected{
	border : 2px solid #0891D4;
}

/*
QHeaderView::section{
	background-color : #171B1F;
	color: #E91E63;	
}
*/

QHeaderView::section {
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                      stop:0 #616161, stop: 0.5 #505050,
                                      stop: 0.6 #434343, stop:1 #656565);
    color: white;
    padding-left: 4px;
    border: 0px solid #6c6c6c;
}

/* Scroll bar styles */

QScrollBar:horizontal{
	height : 0px;
}

QScrollBar:vertical{
	margin-left : 1px;
	width : 25px;
	background-color : #171B1F;	
	border: 1px solid #121212;
}

QScrollBar::handle:vertical{
	background : qlineargradient( spread:Reflect, x1:0, y1:0, x2:0, y2:1 , stop:0 #171B1F, stop:0.5 #38424C, stop:1 #171B1F);
	width : 25px;
	min-height : 500px;
	margin-left : 0px;
	margin-top : 1px;
	margin-bottom : 1px;
	margin-right : 0px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
	width : 0px;
	height : 0px;
}

